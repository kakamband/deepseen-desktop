{
  "name": "vue-electron-audioplayer",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Peter Dyumin",
    "url": "https://github.com/peterdee"
  },
  "description": "A very simple audio player with Vue and Electron",
  "homepage": "https://github.com/peterdee/vue-electron-audioplayer",
  "main": "background.js",
  "scripts": {
